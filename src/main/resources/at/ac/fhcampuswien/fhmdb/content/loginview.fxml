<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="331.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.fhmdb.ui.controller.LoginViewController">
    <VBox prefHeight="331.0" prefWidth="570.0" style="-fx-background-color: black;" VBox.vgrow="ALWAYS">
        <padding>
            <Insets bottom="25.0" left="150" top="30.0" />
        </padding>
        <children>
            <TextField fx:id="username" maxWidth="150.0" prefHeight="26.0" prefWidth="115.0" promptText="User Name" translateX="50.0" translateY="93.0">
            <VBox.margin>
               <Insets />
            </VBox.margin></TextField>
            <Label styleClass="text-red" stylesheets="@../styles/styles.css" text="username error" translateX="50.0" translateY="93.0">
            <font>
                <Font size="8.0" />
            </font>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <VBox.margin>
               <Insets />
            </VBox.margin>
            </Label>
            <TextField fx:id="Password" maxWidth="150.0" prefHeight="20.0" prefWidth="70.0" promptText="password" translateX="50.0" translateY="93.0">
            <VBox.margin>
               <Insets />
            </VBox.margin></TextField>
            <Label styleClass="text-red" stylesheets="@../styles/styles.css" text="password error" translateX="50.0" translateY="93.0">
                <font>
                    <Font size="8.0" />
                </font>
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
            <VBox.margin>
               <Insets />
            </VBox.margin>
            </Label>

        </children>
    </VBox>

    </AnchorPane>
